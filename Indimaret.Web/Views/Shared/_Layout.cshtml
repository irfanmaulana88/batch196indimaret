<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/datatable/datatables.min.css" rel="stylesheet" type="text/css"/> @*datatable*@
    <link href="~/datepicker/jquery-ui.min.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>

    <script src="~/Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/datatable/datatables.min.js"></script> @*datatable*@
    
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("PT. XSIS", "Index", "Home", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li> @*nama navbar, nama cshtml , nama folder yg dituju*@
                    <li>@Html.ActionLink("Transaksi", "Sample", "Transaction")</li>
                    <li>@Html.ActionLink("Barang", "Index", "Barang")</li>
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Create By Irfan Maulana</p>
        </footer>
    </div>


    @*<script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>*@

    @*<script>
    $(Document).ready(function(){
        $('#DataTable').dataTable({

        });
    });

    </script>*@
    <script src="~/datepicker/jquery-ui.min.js"></script>
    <script src="~/mask/jquery.mask.min.js"></script>
</body>
</html>